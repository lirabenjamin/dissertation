@article{veen_conflict_2006,
	title = {Conflict and {Cognitive} {Control} in the {Brain}},
	volume = {15},
	issn = {0963-7214, 1467-8721},
	url = {http://journals.sagepub.com/doi/10.1111/j.1467-8721.2006.00443.x},
	doi = {10.1111/j.1467-8721.2006.00443.x},
	abstract = {Recent research from cognitive psychology and cognitive neuroscience has suggested that the control mechanisms by which people are able to regulate task performance can be dissociated into evaluative and executive components. One process, implemented in the anterior cingulate cortex of the brain, monitors the amount of conﬂict that occurs during information processing; another process, implemented in the dorsolateral prefrontal cortex, is involved with maintaining the requirements of the task at hand and with biasing information processing in favor of appropriate responses. In the current article, we review this theory and some of the research that has supported it, including its implication for understanding cognitive disturbances in clinical disorders such as schizophrenia and obsessive-compulsive disorder. We conclude by addressing several interesting possibilities for future research.},
