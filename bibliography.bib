  author = {Borges, Beatriz and Foroutan, Negar and Bayazit, Deniz and Sotnikova, Anna and Montariol, Syrielle and Nazaretzky, Tanya and Banaei, Mohammadreza and Sakhaeirad, Alireza and Servant, Philippe and Neshaei, Seyed Parsa and Frej, Jibril and Romanou, Angelika and Weiss, Gail and Mamooler, Sepideh and Chen, Zeming and Fan, Simin and Gao, Silin and Ismayilzada, Mete and Paul, Debjit and Schwaller, Philippe and Friedli, Sacha and Jermann, Patrick and K{\"a}ser, Tanja and Bosselut, Antoine and {EPFL Grader Consortium} and {EPFL Data Consortium}},
  year = {2024},
