@article{dora_daily_2023,
	title = {The daily association between affect and alcohol use: {A} meta-analysis of individual participant data.},
	volume = {149},
	issn = {1939-1455(Electronic),0033-2909(Print)},
	doi = {10.1037/bul0000387},
	abstract = {Influential psychological theories hypothesize that people consume alcohol in response to the experience of both negative and positive emotions. Despite two decades of daily diary and ecological momentary assessment research, it remains unclear whether people consume more alcohol on days they experience higher negative and positive affects in everyday life. In this preregistered meta-analysis, we synthesized the evidence for these daily associations between affect and alcohol use. We included individual participant data from 69 studies (N = 12,394), which used daily and momentary surveys to assess the affect and the number of alcoholic drinks consumed. Results indicate that people are not more likely to drink on days they experience high negative affect but are more likely to drink and drink heavily on days high in positive affect. People self-reporting a motivational tendency to drink-to-cope and drink-to-enhance consumed more alcohol but not on days they experienced higher negative and positive affects. Results were robust across different operationalizations of affect, study designs, study populations, and individual characteristics. These findings challenge the long-held belief that people drink more alcohol following increase in negative affect. Integrating these findings under different theoretical models and limitations of this field of research, we collectively propose an agenda for future research to explore open questions surrounding affect and alcohol use. (PsycInfo Database Record (c) 2023 APA, all rights reserved)},
	number = {1-2},
	journal = {Psychological Bulletin},
	author = {Dora, Jonas and Piccirillo, Marilyn and Foster, Katherine T. and Arbeau, Kelly and Armeli, Stephen and Auriacombe, Marc and Bartholow, Bruce and Beltz, Adriene M. and Blumenstock, Shari M. and Bold, Krysten and Bonar, Erin E. and Braitman, Abby and Carpenter, Ryan W. and Creswell, Kasey G. and De Hart, Tracy and Dvorak, Robert D. and Emery, Noah and Enkema, Matthew and Fairbairn, Catharine E. and Fairlie, Anne M. and Ferguson, Stuart G. and Freire, Teresa and Goodman, Fallon and Gottfredson, Nisha and Halvorson, Max and Haroon, Maleeha and Howard, Andrea L. and Hussong, Andrea and Jackson, Kristina M. and Jenzer, Tiffany and Kelly, Dominic P. and Kuczynski, Adam M. and Kuerbis, Alexis and Lee, Christine M. and Lewis, Melissa and Linden-Carmichael, Ashley N. and Littlefield, Andrew and Lydon-Staley, David M. and Merrill, Jennifer E. and Miranda Jr., Robert and Mohr, Cynthia and Read, Jennifer P. and Richardson, Clarissa and O'Connor, Roisin M. and O'Malley, Stephanie S. and Papp, Lauren and Piasecki, Thomas M. and Sacco, Paul and Scaglione, Nichole and Serre, Fuschia and Shadur, Julia and Sher, Kenneth J. and Shoda, Yuichi and Simpson, Tracy L. and Smith, Michele R. and Stevens, Angela and Stevenson, Brittany and Tennen, Howard and Todd, Michael and Treloar Padovano, Hayley and Trull, Timothy and Waddell, Jack and Walukevich-Dienst, Katherine and Witkiewitz, Katie and Wray, Tyler and Wright, Aidan G. C. and Wycoff, Andrea M. and King, Kevin M.},
