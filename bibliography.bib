@article{kuncel_validity_2005,
	title = {The validity of self-reported grade point averages, class ranks, and test scores: {A} meta-analysis and review of the literature},
	volume = {75},
	issn = {0034-6543, 1935-1046},
	shorttitle = {The {Validity} of {Self}-{Reported} {Grade} {Point} {Averages}, {Class} {Ranks}, and {Test} {Scores}},
